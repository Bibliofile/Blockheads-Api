{
  "name": "blockheads-api",
  "version": "0.3.6",
  "description": "APIs for interacting with cloud servers and mac servers without requiring changes to your code.",
  "main": "cloud.js",
  "types": "could.d.ts",
  "repository": "https://github.com/bibliofile/blockheads-api",
  "author": "Bibliofile <bibliofilegit@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "simple-plist": "^0.2.1",
    "typed-promisify": "^0.4.0"
  },
  "devDependencies": {
    "@std/esm": "^0.11.3",
    "ava": "^0.22.0",
    "nyc": "^11.2.1",
    "tslint": "^5.7.0",
    "typedoc": "0.8.0",
    "typescript": "^2.5.3"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc ava",
    "report": "nyc report --reporter=html --reporter=lcov",
    "docs": "typedoc --out docs --exclude \"**/{logs/*,sha1.ts,*test.ts}\" --excludePrivate --excludeNotExported && echo Save >> docs/.nojekyll",
    "prepare": "npm run build"
  },
  "nyc": {
    "require": [
      "@std/esm"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  }
}
